<!DOCTYPE html>
<html>
  <head>
    <style>
      #map {
        height: 400px;
        width: 70%;
       }

       }
    </style>
  </head>
  <body>
    <h3>Place your marker on the map</h3>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?AIzaSyDG3tP6iZ6Jy-rqgHBKXJ_sXcQbeO4qjJU&callback=initMap">
    </script>
  </body>
</html>
