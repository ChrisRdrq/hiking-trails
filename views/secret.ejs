<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" lang="en">
    <title>Secrect</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <h3>Place your marker on the map</h3>
    <div id='outerMap'>
      <div id="map"></div>
    </div>
    <script>
      function initMap() {
        var uluru = {lat: 33.7490, lng: -84.3880};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDG3tP6iZ6Jy-rqgHBKXJ_sXcQbeO4qjJU&callback=initMap">
    </script>
  </body>
</html>
